{
    "name": "u1js",
    "version": "0.0.1",
    "description": "Core Library for UINL",
    "main": "dist/main.js",
    "scripts": {
        "dev": "nodemon",
        "build": "tsc",
        "lint": "eslint src",
        "test": "tsc && NODE_OPTIONS=--experimental-vm-modules jest --verbose",
        "format": "prettier --write src",
        "prepare": "husky"
    },
    "keywords": [
        "UI Library"
    ],
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.9.1",
        "@jest/globals": "^29.7.0",
        "@types/eslint__js": "^8.42.3",
        "eslint": "^9.9.1",
        "husky": "^9.1.5",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.10",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "typescript": "^5.5.4",
        "typescript-eslint": "^8.4.0"
    },
    "lint-staged": {
        "*.ts": [
            "prettier --write",
            "eslint"
        ]
    }
}
